#include <iostream>
using namespace std;

int main ()
{
int number;
cout << "Enter a Number:
cin >> number;
if (number X 20)
cout << "This is an Even number" << endl;
else
cout<<< "This is an Odd number" << endl;
if (number <8)
{
cout << "Negative" <<<<< endl;
}
else if (number > 0)
{
cout << "Positive" << endl;
}
else
{
cout << "Zero" << endl;
}
return 0;
}
